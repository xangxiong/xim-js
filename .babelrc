{
  "presets": ["react", "stage-1"],
  "env": {
    "development": {
      "presets": ["latest"]
    },
    "production": {
      "presets": ["latest", { "env": { "modules": false } }],
	  "plugins": [
        "transform-react-constant-elements",
        "transform-react-remove-prop-types"
	  ]
    },
	"test": {
	  "presets": [
		"latest"
	  ]
	}
  }
}